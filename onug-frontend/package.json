{
  "name": "onug-frontend",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "vite",
    "start:prod": "vite preview --host",
    "build": "tsc && vite build",
    "build:debug": "cross-env DEBUG=vite-tsconfig-paths yarn vite",
    "build:prod": "tsc && vite build --mode prod",
    "serve": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage --watch=false"
  },
  "dependencies": {
    "@vitejs/plugin-react-swc": "^3.7.1",
    "fs": "^0.0.1-security",
    "mobx": "^6.13.5",
    "mobx-react-lite": "^4.0.7",
    "path": "^0.12.7",
    "react": "18",
    "react-dom": "18",
    "react-router-dom": "^6.27.0",
    "react-use": "^17.5.1",
    "react-use-websocket": "^4.9.0",
    "styled-components": "^6.1.13",
    "vite": "^5.4.9",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vite-typescript-plugin": "^0.3.0",
    "vitest": "^2.1.3"
  },
  "devDependencies": {
    "@types/node": "^22.7.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/parser": "^8.9.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "One Night Ultimate Werewolf - Frontend",
  "author": "Rika",
  "license": "MIT"
}
