    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint-plugin-emotion": "^11.0.0",
    "mobx": "^6.12.0",
    "mobx-react": "^9.1.0",
    "mobx-react-lite": "^4.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "react-use-websocket": "3.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
