    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-mobx": "^0.0.9",
    "eslint-plugin-prettier": "^5.0.1",
    "jsdom": "^24.0.0",
    "prettier": "3.1.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "vitest": "^1.3.0"
  },
  "browserslist": {
    "production": [
